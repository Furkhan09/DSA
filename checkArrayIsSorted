#include <bits/stdc++.h>
using namespace std;
bool isSorted(int arr[],int a)
{
  for(int i=1;i<a;i++)
  {
    if(arr[i]<arr[i-1])
    {
      return false;
    }
  }
  return true;
}

int main() 
{
    int arr[] = {3,7,23,15,27};
    if(isSorted(arr,5))
    {
      cout<<"isSorted"<<endl;
    }
    else
    {
      cout<<"notSorted"<<endl;
    }
    return 0;
}
